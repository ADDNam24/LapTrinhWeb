@model IEnumerable<NguyenHoangNam.Models.NHAXUATBAN>

<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">SÁCH THEO NHÀ XUẤT BẢN</li>
    @if (Model != null)
    {
        foreach (var item in Model)
        {
           <a href="~/NguyenHoangNam/SachTheoNhaXuatBan?id=@item.MaNXB" class="list-group-item">@item.TenNXB</a>
        }
    }
</ul>